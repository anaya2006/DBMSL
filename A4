2nd IMPLEMENTATION

CREATE TABLE CIRCLE (RADIUS FLOAT (5) ,  AREAS FLOAT (5)); 

    DECLARE
     r FLOAT ;
     a FLOAT ;

     BEGIN 
        FOR i IN 5..9 
         LOOP
         a := 3.14*i*i;
         INSERT INTO CIRCLE (RADIUS, AREAS) VALUES (i,a);
         END LOOP;
    END;
    /
    
    
