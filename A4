     CREATE TABLE borrower (
    roll NUMBER(3) ,
    sname VARCHAR2(10),
    doi DATE,
    bname VARCHAR2(10),
    bstatus VARCHAR2(1)
    );

CREATE TABLE fine (
     roll NUMBER(3),
     idate DATE,
     amt NUMBER(5)
);
